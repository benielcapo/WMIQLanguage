RETURN_DATA Data

root\wmi:
    STORE_FUNC MSAcpi_ThermalZoneTemperature IN Sensors AS list
    STORE_LIST_ELEMENT Sensors[0] IN Sensor
    STORE_PROP Sensor.CurrentTemperature IN Temp
    ARIT ((Temp / 10) - 273.15) IN Celsius
    PRINT "Temperature is "
    PRINT Celsius
    ADD_RETURN Celsius

NULL:
    STORE_FUNC Win32_Processor IN Processors AS list
#    ITERATE_OVER Processors AS Proc STORE_PROP Proc.DeviceID IN DeviceIDs
    ADD_RETURN Processors
RETURN_END Data
